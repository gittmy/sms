{
  "name": "yii2-contrib/sms",
  "description": "Sms module",
  "keywords": [
    "sms",
    "yii2",
    "yii2-sms"
  ],
  "type": "yii2-extension",
  "homepage": "https://github.com/yii2-contrib/sms",
  "license": "MIT",
  "authors": [
    {
      "name": "lichunqiang",
      "email": "light-li@hotmail.com"
    }
  ],
  "require": {
    "php": "~5.6|~7.0",
    "yiisoft/yii2": "^2.0",
    "overtrue/easy-sms": "^0.0.9"
  },
  "suggest": {
    "yiisoft/yii2-redis": "Need for Redis persistence."
  },
  "autoload": {
    "psr-4": {
      "YiiContrib\\Sms\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "YiiContribTest\\Sms\\": "tests"
    }
  },
  "scripts": {
    "test": [
      "php ./vendor/bin/phpunit"
    ]
  },
  "require-dev": {
    "phpunit/phpunit": "~4.0||~5.0||~6.0",
    "yiisoft/yii2-redis": "^2.0"
  },
  "extra": {
    "bootstrap": "YiiContrib\\Sms\\Bootstrap"
  }
}
